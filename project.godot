config_version=4

_global_script_classes=[ {
"base": "",
"class": "GitAPI",
"language": "NativeScript",
"path": "res://addons/godot-git-plugin/git_api.gdns"
}, {
"base": "Line2D",
"class": "StarLine",
"language": "GDScript",
"path": "res://objects/StarLine.gd"
} ]
_global_script_class_icons={
"GitAPI": "",
"StarLine": ""
}

[application]

config/name="ragnarok"
run/main_scene="res://world/World.tscn"
config/icon="res://icon.png"

[autoload]

GameState="*res://GameState.gd"

[display]

window/size/width=1280
window/size/height=720
window/size/always_on_top.debug=true

[editor_plugins]

enabled=PoolStringArray( "res://addons/jamcountdown/plugin.cfg" )

[gdnative]

singletons=[ "res://addons/godot-git-plugin/git_api.gdnlib" ]

[input]

dash={
"deadzone": 0.5,
"events": [ Object(InputEventMouseButton,"resource_local_to_scene":false,"resource_name":"","device":0,"alt":false,"shift":false,"control":false,"meta":false,"command":false,"button_mask":0,"position":Vector2( 0, 0 ),"global_position":Vector2( 0, 0 ),"factor":1.0,"button_index":1,"pressed":false,"doubleclick":false,"script":null)
 ]
}

[layer_names]

2d_physics/layer_1="Player"
2d_physics/layer_2="Obstacle"
2d_physics/layer_3="Boss"

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/fallback_to_gles2=true
quality/intended_usage/framebuffer_allocation=0
quality/intended_usage/framebuffer_allocation.mobile=0
batching/precision/uv_contract=true
environment/default_clear_color=Color( 0.301961, 0.301961, 0.301961, 1 )
environment/default_environment="res://default_env.tres"
